{
  "name": "scribbler",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^6.0.12",
    "electron-packager": "^14.0.6",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "prepack-osx": "yarn build",
    "pack-osx": "electron-packager . --plataform=darwin --out=./dist --overwrite=true --package-manager=yarn"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
